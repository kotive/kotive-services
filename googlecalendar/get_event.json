{
  "name": "Get an event",
  "description": "Get a specific event in Google Calendar.",
  "documentation": "https://developers.google.com/google-apps/calendar/v3/reference/events/get",
  "fields": [
    {
      "label": "Calendar Id",
      "properties": {
          "parameter": "calendarId",
          "fieldType": "text",
          "helptext": "Enter the ID of the calendar where the event is listed."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Event Id",
      "properties": {
          "parameter": "eventId",
          "fieldType": "text",
          "helptext": "Enter the ID of the event you want to see."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Maximum attendees",
      "properties": {
          "parameter": "maxAttendees",
          "fieldType": "number",
          "helptext": "The maximum number of attendees to include in the response."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Time zone",
      "properties": {
          "parameter": "timeZone",
          "fieldType": "text",
          "helptext": "The time zone of the calendar. Formatted as an IANA Time Zone Database name, e.g. 'EST'."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
  ],
  "headers": {
    "Authorization": "Bearer {auth.oauth2Token}"
  },
  "method": "GET",
  "endpoint": "/calendars/{field.calendarId}/events/{field.eventId}?maxAttendees={field.maxAttendees}&timeZone={field.timeZone}",
  "request": {
  },
  "response": {
      "success": {},
      "error": {}
  }
}
