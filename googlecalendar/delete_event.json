{
  "name": "Delete an event",
  "description": "Delete a specific event in Google Calendar.",
  "documentation": "https://developers.google.com/google-apps/calendar/v3/reference/events/delete",
  "fields": [
    {
      "label": "Calendar Id",
      "properties": {
          "parameter": "calendarId",
          "fieldType": "text",
          "helptext": "Enter the ID of the calendar where the event is listed."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Event Id",
      "properties": {
          "parameter": "eventId",
          "fieldType": "text",
          "helptext": "Enter the ID of the event you want to delete."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Send notifications?",
      "properties": {
        "parameter": "sendNotifications",
        "fieldType": "radio",
        "choices": [
            {"key": "true", "value": "Yes"},
            {"key": "false", "value": "No"}
        ],
        "default": "false",
        "classes": "inline",
        "helptext": "Should notifications of the deletion of the event be sent?"
      },
      "readonly": false,
      "required": false,
      "visible": true
    }
  ],
  "headers": {
    "Authorization": "Bearer {auth.oauth2Token}"
  },
  "method": "DELETE",
  "endpoint": "/calendars/{field.calendarId}/events/{field.eventId}?sendNotifications={string_to_boolean(field.sendNotifications)}",
  "request": {
  },
  "response": {
      "success": {},
      "error": {}
  }
}
