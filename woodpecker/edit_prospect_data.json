{
  "name": "Edit prospect data",
  "description": "Edit prospect data.",
  "documentation": "http://help.woodpecker.co/article/16-api-docs",
  "fields": [
    {
      "label": "Update",
      "properties": {
        "parameter": "update",
        "fieldType": "radio",
        "choices": [
          {"key": "True", "value": "Yes"},
          {"key": "False", "value": "No"}
        ],
        "default": "True",
        "classes": "inline",
        "helptext": "If the prospect already exsist, do you want to update the existing details 'Yes', or if you want to keep it as is select 'No'."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Prospect id",
      "properties": {
        "parameter": "prospectId",
        "fieldType": "text",
        "helptext": "Provide the id of the prospect's data you want to edit."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Email",
      "properties": {
        "parameter": "email",
        "fieldType": "email",
        "helptext": "Email address of the prospect you want to add the the campaign."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Title",
      "properties": {
        "parameter": "tags",
        "fieldType": "text",
        "helptext": "An list of objects with key + values representing a record in your dataset."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Firstname",
      "properties": {
        "parameter": "fname",
        "fieldType": "text",
        "helptext": "Firstname of the prospect you want to add the the campaign."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Lastname",
      "properties": {
        "parameter": "lname",
        "fieldType": "text",
        "helptext": "Firstname of the prospect you want to add the the campaign."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Company",
      "properties": {
        "parameter": "tags",
        "fieldType": "text",
        "helptext": "Name of the company."
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Industry",
      "properties": {
        "parameter": "tags",
        "fieldType": "text",
        "helptext": "Which industry are they in?"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Phone number",
      "properties": {
        "parameter": "phone",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Address",
      "properties": {
        "parameter": "phone",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "City",
      "properties": {
        "parameter": "phone",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "State",
      "properties": {
        "parameter": "phone",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Country",
      "properties": {
        "parameter": "country",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Website",
      "properties": {
        "parameter": "website",
        "fieldType": "text",
        "helptext": "Prospect's website address"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Status",
      "properties": {
        "parameter": "status",
        "fieldType": "select",
        "choices": [
          {"key": "ACTIVE", "value": "Active"},
          {"key": "BLACKLIST", "value": "Blacklist"},
          {"key": "AUTOREPLIED", "value": "Autoreplied"},
          {"key": "TO-CHECK", "value": "To-check"},
          {"key": "TO-REVIEW", "value": "To-review"},
          {"key": "BOUNCED", "value": "Bounced"},
          {"key": "INVALID", "value": "Invalid"},
          {"key": "REPLIED", "value": "Replied"},
          {"key": "PAUSED", "value": "Paused"}
        ],
        "default": "ACTIVE",
        "helptext": "Select the status of the prospect."
      },
      "readonly": false,
      "required": true,
      "visible": true
    },
    {
      "label": "Tags",
      "properties": {
        "parameter": "tags",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Snippet",
      "properties": {
        "parameter": "snippet1",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Snippet",
      "properties": {
        "parameter": "snippet2",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Snippet",
      "properties": {
        "parameter": "snippet3",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    },
    {
      "label": "Snippet",
      "properties": {
        "parameter": "snippet4",
        "fieldType": "text"
      },
      "readonly": false,
      "required": false,
      "visible": true
    }
  ],
  "headers": {
    "Authorization": "Basic {base64_encode_username_password(auth.field.apiAccessKey:)}",
    "Content-Type": "application/json"
  },
  "method": "POST",
  "endpoint": "/rest/v1/add_prospects_list",
  "request": {
    "update": "{string_to_boolean(field.update)}",
    "prospects": [
      {
        "id":"{field.prospectId}",
        "email":"{field.email}",
        "title":"{field.title}",
        "first_name":"{field.fname}",
        "last_name":"{field.lname}",
        "company":"{field.company}",
        "industry":"{field.industry}",
        "phone":"{field.phone}",
        "address":"{field.address}",
        "city":"{field.city}",
        "state":"{field.state}",
        "country":"{field.country}",
        "website":"{field.website}",
        "status":"{string_to_boolean(field.status)}",
        "tags":"{field.tags}",
        "snipet1":"{field.snippet1}",
        "snipet2":"{field.snippet2}",
        "snipet3":"{field.snippet3}",
        "snipet4":"{field.snippet4}"
      }
    ]
  },
  "response": {
    "success": {},
    "error": {}
  }
}
