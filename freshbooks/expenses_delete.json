{
    "name": "Delete an expense",
    "description": "Delete an expense in freshbooks.",
    "documentation": "https://www.freshbooks.com/api/payments",
    "fields": [
        {
            "label": "Expense id",
            "properties": {
                "parameter": "expenseId",
                "fieldType": "text",
                "helptext": "The ID of the expense you want to delete."
            },
            "readonly": false,
            "required": true,
            "visible": true
        }
    ],
    "headers": {
      "Authorization": "Bearer {auth.oauth2Token}"
    },
    "method": "PUT",
    "endpoint": "/accounting/account/{auth.field.apikey}/expenses/expenses/{field.expenseId}",
    "request": {
        "expense": {
            "vis_state": 1
        }
    },
    "response": {
        "success": "",
        "error": {}
    }
}
