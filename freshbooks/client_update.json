    "method": "PUT",
    "endpoint": "/accounting/account/{auth.field.apikey}/users/clients/{field.clientID}",
    "request": {
        "item": {
            "id": "{field.clientId}",
            "userid": "{field.userId}",
            "accounting_systemid": "{field.accountingSystemId}",
            "fname": "{field.firstname}",
            "lname": "{field.lastname}",
            "email": "{field.email}",
            "bus_phone": "{string_or_null(field.businessPhone)}",
            "home_phone": "{string_or_null(field.homePhone)}",
            "mob_phone": "{string_or_null(field.mobPhone)}",
            "fax": "{field.fax}",
            "organization": "{field.organization}",
            "company_industry": "{string_or_null(field.companyIndustry)}",
            "company_size": "{string_or_null(field.companySize)}",
            "p_street": "{field.billingStreetAddress}",
            "p_street2": "{field.billingStreetAddress2}",
            "p_province": "{field.billingProvince}",
            "p_country": "{field.billingCountry}",
            "p_code": "{field.billingPostalCode}",
            "p_city": "{field.billingCity}",
            "s_street": "{field.shippingStreetAddress}",
            "s_street2": "{field.shippingStreetAddress2}",
            "s_province": "{field.shippingProvince}",
            "s_country": "{field.shippingCountry}",
            "s_code": "{field.shippingPostalCode}",
            "s_city": "{field.shippingCity}",
            "currency_code": "{field.currencyCode}",
            "pref_email": "{string_to_boolean(field.prefEmail)}",
            "pref_gmail": "{string_to_boolean(field.prefGroundMail)}",
            "language": "{field.language}",
            "vat_name": "{string_or_null(field.vatName)}",
            "vat_number": "{string_or_null(field.vatNumber)}",
            "note": "{string_or_null(field.notes)}"
        }
    },
    "response": {
        "success": "",
        "error": {}
    }
