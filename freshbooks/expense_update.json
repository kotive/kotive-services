    "method": "PUT",
    "endpoint": "/accounting/account/{auth.field.apikey}/expenses/expenses/{field.expenseId}",
    "request": {
        "expense": {
            "accounting_systemid": "{field.accountingSystemId}",
            "clientid": "{field.clientId}",
            "categoryid": "{field.categoryId}",
            "projectid": "{field.projectId}",
            "profileid": "{string_or_null(field.profileId)}",
            "invoiceid": "{string_or_null(field.invoiceId)}",
            "accountid": "{string_or_null(field.accountId)}",
            "transactionid": "{string_or_null(field.transactionId)}",
            "ext_systemid": "{field.extSystemId}",
            "ext_invoiceid": "{field.extInvoiceId}",
            "staffid": "{field.staffId}",
            "date": "{field.date}",
            "account_name": "{string_or_null(field.accountName)}",
            "vendor": "{string_or_null(field.vendor)}",
            "bank_name": "{string_or_null(field.bankName)}",
            "amount": {
                "amount": "{field.amount}",
                "code": "{field.currencyCode}"
            },
            "taxName1": "{string_or_null(field.taxName1)}",
            "taxPercent1": "{string_or_null(field.taxPercent1)}",
            "taxAmount1": {
                "amount": "{field.taxAmount1}",
                "code": "{field.taxCurrencyCode1}"
            },
            "taxName2": "{string_or_null(field.taxName2)}",
            "taxPercent2": "{string_or_null(field.taxPercent2)}",
            "taxAmount2": {
                "amount": "{field.taxAmount2}",
                "code": "{field.taxCurrencyCode2}"
            },
            "compounded_tax": "{string_to_boolean(field.compoundedTax)}",
            "isduplicate": "{string_to_boolean(field.isDuplicate)}",
            "has_receipt": "{string_to_boolean(field.hasReceipt)}",
            "note": "{field.notes}"
        }
    },
