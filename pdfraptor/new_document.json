{
    "name": "Create a new document",
    "description": "Create a new document using PDFRaptor",
    "documentation": "https://docraptor.com/documentation",
    "fields": [
        {
            "label": "Document type",
            "properties": {
                "parameter": "document_type",
                "fieldType": "radio",
                "choices": [
                    "xls",
                    "xlsx",
                    "pdf"
                ]
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Document content",
            "properties": {
                "parameter": "document_content",
                "fieldType": "textarea"
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Name",
            "properties": {
                "parameter": "name",
                "fieldType": "text"
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Test",
            "properties": {
                "parameter": "test",
                "fieldType": "radio",
                "choices": [
                    "yes",
                    "no"
                ],
                "default": "no"
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Tag",
            "properties": {
                "parameter": "tag",
                "fieldType": "text"
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Strict",
            "properties": {
                "parameter": "strict",
                "fieldType": "radio",
                "choices": [
                    "none",
                    "html"
                ],
                "default": "false"
            },
            "readonly": false,
            "required": true,
            "visible": true
        },
        {
            "label": "Javascript",
            "properties": {
                "parameter": "javascript",
                "fieldType": "radio",
                "choices": [
                    "yes",
                    "no"
                ],
                "default": "no"
            },
            "readonly": false,
            "required": true,
            "visible": true
        }
    ],
    "method": "POST",
    "endpoint": "",
    "request": {
        "document_type": "{field.document_type}",
        "document_content": "{field.document_content}",
        "name": "{field.name}",
        "test": "{string_to_boolean(field.test)}",
        "tag": "{field.tag}",
        "strict": "{string_to_boolean(field.strict)}",
        "javascript": "{string_to_boolean(field.javascript)}"
    },
    "response": {
        "success": {
            "Code": 200,
            "Message": "Ok"
        },
        "error": {
            "Code": 400,
            "Message": "Bad Request"
        }
    }
}
