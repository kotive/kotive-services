{
    "name": "PDFRaptor",
    "description": "DocRaptor makes it easy to create PDF and XLS files on the fly.",
    "auth": {
        "method": "Custom",
        "fields": {
            "label": "API Key"
            "properties": {
                "parameter": "apikey",
                "fieldType": "text"
            },
            "readonly": false,
            "required": true,
            "visible": true
        }
    },
    "domain": "https://docraptor.com/docs?user-credentials={auth.fields.apikey}"
}
